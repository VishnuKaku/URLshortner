{
    "name": "url-shortener",
    "version": "1.0.0",
    "description": "A custom URL shortener with advanced analytics and features.",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "nodemon src/server.ts",
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/*.ts",
        "lint:fix": "eslint src/*.ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\""
    },
    "dependencies": {
        "@types/express": "^4.17.17",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^7.5.2",
        "redis": "^4.6.7",
        "typescript": "^5.1.6",
        "ua-parser-js": "^2.0.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/ioredis": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^20.3.2",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.1.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.7.0",
        "mongodb-memory-server": "^8.12.2",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1"
    }
}
